{
  "branches": [
     "main",
     {"name": "hotfix/*", "prerelease": "hotfix"},
     {"name": "rc/*", "prerelease": "rc"}
  ],
  "tagFormat": "${version}",
  "plugins": [
    "@semantic-release/commit-analyzer",
    ["@semantic-release/changelog", {
      "changlogFile": "changelog.md"
    }],
    ["@semantic-release/git", {
      "assets": "changelog.md"
    }]
  ]
}
